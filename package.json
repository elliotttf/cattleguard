{
  "name": "cattleguard",
  "version": "1.0.1",
  "description": "Rate limiting stampede protection for express.",
  "main": "index.js",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "test": "istanbul cover --print both nodeunit test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliotttf/cattleguard.git"
  },
  "keywords": [
    "express",
    "stampede",
    "rate",
    "limit"
  ],
  "author": "Elliott Foster <elliottf@codebrews.com> (http://codebrews.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elliotttf/cattleguard/issues"
  },
  "homepage": "https://github.com/elliotttf/cattleguard#readme",
  "devDependencies": {
    "eslint": "^3.6.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "coveralls": "^2.11.14",
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "node-cache": "^4.1.0",
    "nodeunit": "^0.10.2",
    "sinon": "^1.17.6"
  },
  "engines": {
    "node": ">=6"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  }
}
